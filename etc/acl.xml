<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::system">
                    <resource id="Magefan_Security::elements" title="Security Elements" sortOrder="10">
                        <resource id="Magefan_Security::dashboardpage" title="Dashboard" sortOrder="10"/>
                        <resource id="Magefan_Security::refresh" title="Refresh" sortOrder="20"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magefan_Security::config" title="Security Section" sortOrder="10"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
	</acl>
</config>
